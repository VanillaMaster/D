{
    "name": "@builtin/backend",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "types": "./src/global.d.ts",
            "default": null
        },
        "./server": {
            "import": "./src/exports.js"
        },
        "./helpers": {
            "import": "./src/helpers.js"
        }
    },
    "files": [
        "src/**/*",
        "bin/**/*",
        "config.json",
        "schema.json"
    ],
    "bin": {
        "server": "bin/server.js"
    },
    "scripts": {
        "start": "node src/index.js",
        "trace": "node --trace-warnings src/index.js"
    },
    "dependencies": {
        "find-my-way": "^8.1.0",
        "mustache": "^4.2.0",
        "@builtin/module-walker": "*",
        "@builtin/config": "*"
    },
    "devDependencies": {
        "@types/node": "^20.12.7"
    }
}
