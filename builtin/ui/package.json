{
    "name": "@builtin/ui",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "exports": {
        ".": null,
        "./server": {
            "import": "./src/server.js"
        },
        "./client": {
            "import": "./src/client.js"
        },
        
        "./backend": {
            "import": "./src/server.js"
        },
        "./frontend": {
            "import": "./src/client.js"
        }
    },
    "files": [
        "src/**/*",
        "default/**/*",
        "editable/**/*"
    ],
    "dependencies": {
        "@builtin/lit": "*"
    },
    "devDependencies": {
        "@types/node": "^20.12.12"
    },
    "kind": [
        "server",
        "client",
        "backend",
        "frontend"
    ],
    "disabled": false,
    "prefetch": [
        "src/*.html"
    ],
    "editable": [
        "src/*.html"
    ],
    "stylesheet": [
        "src/*.css"
    ]
}